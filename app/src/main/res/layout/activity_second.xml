<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.landayan.labexer3.SecondActivity">

    <TableLayout

        android:layout_width="500dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    <TextView
        android:text="Filename:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText2" />

    <TextView
        android:id="@+id/tv_display"
        android:text="output"
        android:textSize="15sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_centerHorizontal="true" />

    <Button
        android:onClick="getSharedPreferences"
        android:id="@+id/btn_SharedPreferences"
        android:text="Shared preferences"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:onClick="getInternalStorage"
        android:id="@+id/btn_InternalStorage"
        android:text="Internal storage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:onClick="getInternalCache"
        android:id="@+id/btn_InternalCache"
        android:text="internal cache"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_data"
        android:layout_centerHorizontal="true" />

    <Button
        android:onClick="getExternalCache"
        android:id="@+id/btn_ExternalCache"
        android:text="External Cache"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_InternalCache"
        android:layout_alignRight="@+id/btn_InternalCache"
        android:layout_alignEnd="@+id/btn_InternalCache" />

    <Button
        android:onClick="getExternalStorage"
        android:id="@+id/btn_ExternalStorage"
        android:text="External Storage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_ExternalCache"
        android:layout_centerHorizontal="true" />

    <Button
        android:onClick="getExtPublicStorage"
        android:id="@+id/btn_ExtPublicStorage"
        android:text="Ext Public Storage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_ExternalStorage"
        android:layout_centerHorizontal="true" />

    <Button
        android:onClick="Back"
        android:id="@+id/btn_next"
        android:text="Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_ExtPublicStorage"
        android:layout_centerHorizontal="true" />

    </TableLayout>
</RelativeLayout>
